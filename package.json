{
  "name": "healthcare-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "node scripts/build.js",
    "build:dev": "node scripts/build.js development",
    "build:staging": "node scripts/build.js staging",
    "build:production": "node scripts/build.js production",
    "build:legacy": "pnpm run pre-build && nest build",
    "pre-build": "pnpm run validate:all",
    "validate:all": "pnpm run prisma:validate && pnpm run env:validate && pnpm run type-check && pnpm run lint:check && pnpm run security:audit && pnpm run format:check && pnpm run deps:check && pnpm run outdated:check && pnpm run todo:check",
    "validate:build": "pnpm run validate:all && pnpm run build:test",
    "build:test": "tsc --noEmit --incremental false",
    "type-check": "tsc --noEmit",
    "lint": "pnpm exec eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "lint:check": "pnpm exec eslint \"{src,apps,libs}/**/*.ts\" --max-warnings 0",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "security:audit": "pnpm audit --audit-level=moderate",
    "prisma:validate": "prisma format --schema=./src/libs/infrastructure/database/prisma/schema.prisma --check",
    "env:validate": "ts-node -r tsconfig-paths/register scripts/validate-env.ts",
    "deps:check": "pnpm exec depcheck --ignores='@nestjs/*,@types/*,eslint-*,prettier,typescript,ts-node,tsconfig-paths,prisma,@prisma/client,glob,js-yaml' || echo 'Dependency check completed'",
    "outdated:check": "node scripts/check-outdated.js || echo 'Outdated dependencies check completed'",
    "todo:check": "node scripts/check-todos.js || echo 'TODO check skipped (script may not exist)'",
    "pre-commit": "pnpm run validate:all",
    "pre-push": "pnpm run validate:build",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch --watchAssets",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "start:prod:cluster": "cross-env NODE_ENV=production ENABLE_CLUSTERING=true node dist/main.js",
    "prisma:generate": "prisma generate --schema=./src/libs/infrastructure/database/prisma/schema.prisma && node scripts/fix-prisma-pnpm.js && ts-node -r tsconfig-paths/register scripts/fix-prisma-types.ts && pnpm exec prettier --write \"src/libs/infrastructure/database/prisma/generated/**/*.ts\"",
    "postinstall": "pnpm prisma:generate",
    "prisma:migrate": "prisma migrate deploy --schema=./src/libs/infrastructure/database/prisma/schema.prisma",
    "prisma:studio": "prisma studio --schema=./src/libs/infrastructure/database/prisma/schema.prisma",
    "prisma:format": "prisma format --schema=./src/libs/infrastructure/database/prisma/schema.prisma",
    "prisma:reset": "prisma migrate reset --schema=./src/libs/infrastructure/database/prisma/schema.prisma --force",
    "seed:dev": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/libs/infrastructure/database/prisma/seed.ts",
    "seed:prod": "cross-env NODE_ENV=production node dist/libs/infrastructure/database/prisma/seed.js",
    "deploy:dev": "pnpm run prisma:generate && pnpm run prisma:migrate && pnpm run seed:dev",
    "deploy:prod": "pnpm run build && pnpm run prisma:generate && pnpm run prisma:migrate && pnpm run seed:prod",
    "docker:up": "cross-env NODE_ENV=development docker-compose -f devops/docker/docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f devops/docker/docker-compose.dev.yml down",
    "docker:logs": "docker-compose -f devops/docker/docker-compose.dev.yml logs -f",
    "docker:rebuild": "docker-compose -f devops/docker/docker-compose.dev.yml down && cross-env NODE_ENV=development docker-compose -f devops/docker/docker-compose.dev.yml up -d --build",
    "docker:prod": "cross-env NODE_ENV=production docker-compose -f devops/docker/docker-compose.prod.yml up -d --build",
    "k8s:deploy": "powershell -ExecutionPolicy Bypass -File devops/kubernetes/scripts/deploy-local.ps1",
    "k8s:teardown": "powershell -ExecutionPolicy Bypass -File devops/kubernetes/scripts/teardown-local.ps1",
    "k8s:logs": "kubectl logs -f deployment/healthcare-api -n healthcare-backend",
    "k8s:status": "kubectl get pods,svc,deployments -n healthcare-backend",
    "health:check": "curl -f http://localhost:8088/health || echo 'Health check failed'"
  },
